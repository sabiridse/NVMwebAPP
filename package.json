{
  "name": "autostart",
  "version": "1.0.0",
  "description": "autoStart all services",
  "scripts": {
    "server": "cd server && npm run dev",
    "client": "cd client && npm run serve",
    "start": "concurrent \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^1.0.0"
  }
}
